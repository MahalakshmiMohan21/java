public class SymbolPattern {
    public static void main(String[] args) {
        int rows = 3;
        char[] symbols = {'@', '?', '>'};

        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < 3; j++) {
                System.out.print(symbols[i] + "  ");
            }
            System.out.println();
        }
    }
}

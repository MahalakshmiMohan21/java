
package alphapattern;
import java.util.*;
public class Alphapattern {
    public static void main(String[] args) {
       String str="alpha";
       for(int i=1;i<str.length();i++){
           for(int j=0;j<i;j++){
               System.out.print(str.charAt(j)+" ");
           }
           System.out.println();
       }
    }
    
}
